                                        <input type="file" class="form-control input-sm" id="fileUpload_Job_Application_Image" placeholder="Upload Job_Application Image" name="">

                                        <button type="button" class="btn btn-default btn-sm" id="btnUpload">Upload Image</button>
                                    </div>

                                    <div class="form-group">

                                        <label style="color: #333">

                                            @if (Model.Job_Application.Is_Active == true)
                                            {
                                                <input type="checkbox" id="chkIsActive" checked />@:&nbsp;&nbsp;&nbsp;&nbsp; Status
                                                }
                                            else
                                            {
                                                <input type="checkbox" id="chkIsActive" />@:&nbsp;&nbsp;&nbsp;&nbsp; Status
                                                }

                                            <input type="hidden" id="hdnJob_Application_Status" name="Job_Application.Is_Active" value="@Model.Job_Application.Is_Active">
                                        </label>

                                    </div>*@

                                <div class="btn-group">

                                    @*<button type="button" class="btn btn-default btn-sm" id="btnSave">Save</button>

                                        <button type="button" class="btn btn-default btn-sm" id="btnReset">Reset</button>*@

