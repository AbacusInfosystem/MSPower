@model MSPowerWebApp.Models.Job_ApplicationViewModel

  
@{
    Layout = null;
}

<!DOCTYPE html>

<html>

<head>

    <meta name="viewport" content="width=device-width" />

    <title>Index</title>

    <script src="~/Content/AdminLTE/plugins/jQuery/jQuery-2.1.3.min.js"></script>

    <script src="~/Scripts/jquery.validate.js"></script>

    <script src="~/Scripts/Job_Application/job_application_events.js"></script>

    <script src="~/Scripts/Job_Application/job_application_method.js"></script>

    <script src="~/Scripts/Job_Application/job_application_validation.js"></script>

</head>

<body>

    @{Html.RenderPartial("_Sidebar_Online");}

    <div>
        Contact Us. @Session["Language"].ToString();

        <div class="row">

            <div class="col-xs-12">

                <div class="box">

                    <div class="box-body">

                        <div class="row">

                            <div class="col-md-12">

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtReference">Reference : @Model.Job_Application.Reference</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtAdditional_Information">Additional Information : @Model.Job_Application.Additional_Information</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtCreated_On">Created On : @Model.Job_Application.Created_On</label>

                                </div>

                                @if (Model.Job_Application.Is_DOCX_Exists)
                                {
                                    <div class="form-group">

                                        <input type="button" id="btnDownloadDOCX" class="btn btn-sm" value="Download DOCX" />

                                    </div>
                                }

                                <div class="btn-group">

                                    <input type="hidden" name="Job_Application.Job_Application_Id" id="hdnJob_Application_Id" value="@Model.Job_Application.Job_Application_Id">

                                    @*<input type="hidden" name="Job_Application.Job_Application_Id" id="hdfJob_Application_Id" value="@Model.Job_Application.Job_Application_Id">*@

                                </div>

                            </div>

                        </div>

                    </div>
                    <!-- /.box-body -->

                </div>
                <!-- /.box -->

            </div>

        </div>

    </div>
</body>

</html>

