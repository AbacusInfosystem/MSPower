<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <title>MS Power - Semiconductor | Home</title>

    @*<link href="../../../ContentOnline/css/bootstrap.css" rel='stylesheet' type='text/css' />*@
    <link href="../../../Content/AdminLTE/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Custom Theme files -->
    <link href="../../../ContentOnline/css/style.css" rel="stylesheet" type="text/css" media="all" />

    <link href="../../../ContentOnline/css/animate.css" rel='stylesheet' type='text/css' />

    @*<link href="../ContentOnline/plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" />

    <script src="../ContentOnline/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>*@

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

    <!-- Custom Theme files -->

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="United Comms Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template,Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    @*<script src="../../../ContentOnline/js/jquery.min.js"></script>*@
    <script src="../../../Content/AdminLTE/plugins/jQuery/jQuery-2.1.3.min.js"></script>
    <script src="../../../Content/AdminLTE/bootstrap/js/bootstrap.min.js"></script>
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>

    @*<script src="../../../ContentOnline/js/wow.min.js"></script>

    <script>
        new WOW().init();
    </script>*@


</head>
<body>
    <div class="header">
        <div class="container">
            <div class="logo">
                <a href="index.html">
                    <img alt="" class="img-responsive" src="../../../ContentOnline/images/mspower.png"></a>
            </div>

            <div class="lang-toggles">
                @if (Session["Language"].ToString() == "en")
                {
                    <a class="lang-toggle lang-en active" href="/en/set-language">
                        <span>English</span>
                    </a>
                }
                else
                {
                    <a class="lang-toggle lang-en" href="/en/set-language">
                        <span>English</span>
                    </a>
                }

                <span class="lang-toggle-divider">|</span>
                @if (Session["Language"].ToString() == "ch")
                {
                    <a class="lang-toggle lang-sp active" href="/ch/set-language">
                        <span>Chinese</span>
                    </a>
                }
                else
                {
                    <a class="lang-toggle lang-sp" href="/ch/set-language">
                        <span>Chinese</span>
                    </a>
                }

            </div>

            <div class="header-right">
                @{Html.RenderPartial("_Sidebar_Online");}
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="search">
            <i class="fa fa-search fa-2x" id="btnModalProductSearch" style="cursor: pointer;"></i>
        </div>
    </div>
   

            @RenderBody()


    <input type="hidden" id="hdfsessionLanguage" value="@Session["Language"].ToString()">
   
    <div class="modal fade" id="div_Parent_Modal_Fade" role="dialog" aria-hidden="true">
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header" id="div_Parent_Modal_Header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">&nbsp;</h4>
                </div>
                <div class="modal-body" id="div_Parent_Modal_Body">

                </div>
            </div>
        </div>
    </div>



    <div class="footer text-center">
        <div class="social-icons">
            <a href="#"><i class="facebook"></i></a>
            <a href="#"><i class="twitter"></i></a>
            <a href="#"><i class="googlepluse"></i></a>
            <a href="#"><i class="youtube"></i></a>
            <a href="#"><i class="linkedin"></i></a>
        </div>
        <div class="copyright">
            <p>Copyright &copy; 2015. All rights reserved | Abacus Infosystem</p>
        </div>
    </div>




</body>
</html>

<script>

    $(function () {

        $("#btnModalProductSearch").click(function () {

            $("#div_Parent_Modal_Fade").find(".modal-title").html("Product Search")

            $("#div_Parent_Modal_Body").load("/" + $("#hdfsessionLanguage").val() + "/get-product-search/", null, function () {

                $("#div_Parent_Modal_Fade").modal("show");
            });
        });
    });
</script>
