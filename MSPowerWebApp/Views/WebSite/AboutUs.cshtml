@model MSPowerWebApp.Models.AboutUsViewModel

@{
    ViewBag.Title = "AboutUs";
}


@*AboutUs. @Session["Language"].ToString();*@




<div class="grid_1">
    <div class="container">
        <div class="box_1">
            <h3>Who we are</h3>
        </div>
        <div class="col-md-12 about_banner"><img src="../../../ContentOnline/images/about.jpg" class="img-responsive" alt="" /></div>
        <div class="box_20">
