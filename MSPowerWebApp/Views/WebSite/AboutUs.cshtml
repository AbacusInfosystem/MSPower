                <p>@Html.Raw(Model.AboutUs.About_Us_Description)</p>
            </div>
            
            <div class="clearfix"> </div>
        </div>
    </div>
</div>

<script>
    $(function () {

        $(".top-menu").find("li").removeClass();

        $("#liAboutUs").addClass("active");

    });
</script>