                                <img src="~/Content/Images/Product%20Categories/Power%20stack.jpg">
                            </div>
                        </li>
                    </ul>
                </section>
                <section class="lazy-load-box  effect-slidefromright">
                    <ul>
                        <li class="wow fadeInRight" data-wow-delay="0.4s">
                            <div class="circle4">
                                <img src="~/Content/Images/Product%20Categories/power%20bridges.JPG">
                            </div>
