<div class="content">

    <div class="container" style="padding-top:40px;">
        <div class="row wow zoomIn" data-wow-delay="0.4s">
            <div class="col-md-3">
                <h3>About</h3>
                <h1 style="border-bottom:5px solid #EE1C25">MS Power</h1>
            </div>
            <div class="col-md-9" id="pAboutUs">
               
            </div>
        </div>
    </div>

    

    <div class="container" style="padding-top: 40px;">
        <div class="row">
            <div class="col-md-4 wow fadeInLeftBig" data-wow-delay="0.4s">
                <div class="panel panel-primary" >
                    <div class="panel-heading" style="text-align: center; background-color: #231F20">Latest News</div>
                    <div class="panel-body" style="padding: 25px 5px 25px 5px;">
                        <ul id="ulLatestNews"></ul>
                    </div>
                </div>
            </div>

            <div class="col-md-4 wow zoomIn" data-wow-delay="0.4s">
                <div class="panel panel-primary" >
                    <div class="panel-heading" style="text-align: center; background-color: #231F20">Hot Jobs</div>
                    <div class="panel-body" style="padding: 25px 5px 25px 5px;">
                        <ul id="ulHotJobs"></ul>
                    </div>
                </div>
            </div>

            <div class="col-md-4 wow fadeInRightBig" data-wow-delay="0.4s">
                <div class="panel panel-primary" >
                    <div class="panel-heading" style="text-align: center; background-color: #231F20">Event</div>
                    <div class="panel-body" style="padding: 25px 5px 25px 5px;">
                        <ul id="ulEvents"></ul>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="grid_1">
        <div class="container">
            <div class="span12 hexagon_box">
                <section class="effect-1">
                    <ul>
                        <li class="wow bounceInRight" data-wow-delay="0.4s">
