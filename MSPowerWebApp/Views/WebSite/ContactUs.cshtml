<style>
    #map {
        height: 100%;
    }
</style>


<div class="grid_4">
    <div class="container">
        <h1 class="blog_head">Contact</h1>

        @foreach (var item in Model.ContactUss)
        {
            string link = "https://www.google.com/maps/embed/v1/place?key=AIzaSyBJBrUCFt7OZ0Twm0qu6xXyKccbZZjjcEY&q=" + item.Latitude + "," + item.Longitude + "&zoom=18";
            <div class="well">
                <div class="row">
                    <div class="col-md-10">
                        <div class="map">
                            <iframe src="@link"></iframe>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <dl>
                            <dd>@item.ContactUs_Title</dd>

                            <dt>@(Html.Raw(item.ContactUs_Address)) </dt>



                        </dl>

                    </div>
                </div>
            </div>
        }
        

    </div>
</div>

             



                <script>
                    $(function () {

                        $(".top-menu").find("li").removeClass();

                        $("#liContactUs").addClass("active");

                    });
                </script>

  
