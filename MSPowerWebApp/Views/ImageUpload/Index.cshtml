@model MSPowerWebApp.Models.ImageUploadViewModel

@{
    ViewBag.Title = "Image Upload";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

     @{Html.RenderPartial("_ServerMessage");}

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Services

            <small>Listing Page</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>CMS</a></li>
            <li class="active">Services Listing</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="row">
            <div class="col-xs-12">
                <div class="box">

                    <div class="box-body no-padding">



                    <form id="frmDragAndDrop" method="POST" enctype = "multipart/form-data" action ="/image-upload/upload"> 
                 <input type="file" name="file" id="file"/><br><br> 


                        <input type="submit" value="Upload Image"/>

                    </form>
                        
</div>
                    </div>
                </div>
            </div>
        </section>

    </div>


