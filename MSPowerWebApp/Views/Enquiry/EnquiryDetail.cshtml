@model MSPowerWebApp.Models.EnquiryViewModel


<script src="~/Scripts/Enquiry/enquiry_search_events.js"></script>

<script src="~/Scripts/Enquiry/enquiry_search_method.js"></script>

@{
    ViewBag.Title = "Enquiry";
}

<!-- Content Wrapper. Contains page content -->

<div class="content-wrapper">

    <!-- Content Header (Page header) -->

    <section class="content-header">

        <h1>
            Enquiry

            <small>Create / Update Page</small>
        </h1>

        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>CMS</a></li>
            <li class="active">Enquiry</li>
        </ol>
    </section>

    <div class="row">

        <div class="col-xs-12">

            <div class="box">

                <div class="box-body">

                    <div class="row">

                        <div class="col-md-12">

                            <form id="frmEnquiry">

                                <div class="form-group">
                                    
                                    <label style="color: #333" for="txtCustomer_Name">Customer Name : @Model.Enquiry.Customer_Name</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtCustomer_Address">Customer Address : @Model.Enquiry.Customer_Address</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtTel_No">Telephone Number : @Model.Enquiry.Tel_No</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtEmail">Email : @Model.Enquiry.Email</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtContact_Person">Contact Person : @Model.Enquiry.Contact_Person</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtContact_Person_Tel_No">Contact Person Telephone No : @Model.Enquiry.Contact_Person_Tel_No</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtContact_Person_Mobile_No">Contact Person Mobile No : @Model.Enquiry.Contact_Person_Mobile_No</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtEnquiry_No">Enquiry No : @Model.Enquiry.Enquiry_No</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtEnquiry_Date">Enquiry Date : @Model.Enquiry.Enquiry_Date</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtProduct_Name">Product Name : @Model.Enquiry.Product_Name</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtQuantity">Quantity : @Model.Enquiry.Quantity</label>

                                </div>

                                <div></div>

                                <div class="form-group">

                                    <label style="color: #333" for="txtDelivery">Delivery : @Model.Enquiry.Delivery</label>

                                </div>

                                <div></div>

                                <div class="form-group">
                                    <label style="color: #333" for="txtCreated_On">Created On : @Model.Enquiry.Created_On</label>

                                </div>


                                @*<div class="form-group">
                                        <label style="color: #333" for="fileUpload_Enquiry_Image">Upload Enquiry Images</label>

                                        <input type="file" class="form-control input-sm" id="fileUpload_Enquiry_Image" placeholder="Upload Enquiry Image" name="">

                                        <button type="button" class="btn btn-default btn-sm" id="btnUpload">Upload Image</button>
                                    </div>

                                    <div class="form-group">

                                        <label style="color: #333">

                                            @if (Model.Enquiry.Is_Active == true)
                                            {
                                                <input type="checkbox" id="chkIsActive" checked />@:&nbsp;&nbsp;&nbsp;&nbsp; Status
                                                }
                                            else
                                            {
                                                <input type="checkbox" id="chkIsActive" />@:&nbsp;&nbsp;&nbsp;&nbsp; Status
                                                }

                                            <input type="hidden" id="hdnEnquiry_Status" name="Enquiry.Is_Active" value="@Model.Enquiry.Is_Active">
                                        </label>

                                    </div>*@

                                <div class="btn-group">

                                    @*<button type="button" class="btn btn-default btn-sm" id="btnSave">Save</button>

                                    <button type="button" class="btn btn-default btn-sm" id="btnReset">Reset</button>*@

                                    @*<a id="btnCancel" class="btn btn-default btn-sm" href="@Url.Action("Search", "Enquiry")" role="button">Cancel</a>*@

                                    @*<button type="button" class="btn btn-default btn-sm" id="btnUploadImage">Upload Image</button>*@

                                    <input type="hidden" name="Enquiry.Enquiry_Id" id="hdnEnquiry_Id" value="@Model.Enquiry.Enquiry_Id">

                                    @*<input type="hidden" name="Enquiry.Enquiry_Id" id="hdfEnquiry_Id" value="@Model.Enquiry.Enquiry_Id">*@

                                </div>

                            </form>
                        </div>

                    </div>

                </div>
                <!-- /.box-body -->

            </div>
            <!-- /.box -->

        </div>

    </div>

    </div>
